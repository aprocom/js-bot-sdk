{
  "name": "@dlghq/dialog-bot-sdk",
  "version": "0.0.0",
  "description": "Dialog Bot SDK",
  "main": "lib/index.js",
  "scripts": {
    "test": "exit 0",
    "api": "pbts -o src/api/index.d.ts node_modules/@dlghq/dialog-api/js/api.js",
    "predocs": "rm -rf docs/",
    "docs": "typedoc --excludePrivate --theme minimal --mode file --out docs src/index.ts",
    "postdocs": "buildbranch gh-pages docs",
    "prebuild": "rm -rf lib/",
    "build": "tsc",
    "preversion": "npm test",
    "postversion": "npm run release",
    "prerelease": "npm run build",
    "release": "git push --follow-tags",
    "postrelease": "npm publish"
  },
  "author": "Nikita Gusakov <gusnkt@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@dlghq/dialog-api": "0.6.3",
    "@grpc/proto-loader": "0.3.0",
    "bluebird": "3.5.2",
    "dataclass": "1.1.1",
    "grpc": "1.15.1",
    "jimp": "0.5.4",
    "lodash": "4.17.11",
    "mime": "2.3.1",
    "node-fetch": "2.2.0",
    "rxjs": "6.3.3",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.24",
    "@types/dotenv": "4.0.3",
    "@types/node-fetch": "2.1.2",
    "@types/uuid": "3.4.4",
    "buildbranch": "2.0.1",
    "dotenv": "6.1.0",
    "nodemon": "1.18.4",
    "ts-node": "7.0.1",
    "typedoc": "0.13.0",
    "typescript": "3.1.3"
  },
  "gh-pages-deploy": {
    "staticpath": "docs"
  }
}
